# Database
DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/ingressa

# AWS scope (locked)
AWS_REGION_DEFAULT=ap-south-1

# Scan execution mode
SCAN_MODE=mock

# AWS auth mode
AWS_AUTH_MODE=keys

# Optional - only for AWS_AUTH_MODE=keys
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=

# Optional - only for AWS_AUTH_MODE=assume_role
AWS_ASSUME_ROLE_ARN=
